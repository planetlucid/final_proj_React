<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Image url</th>
      <th>Genre</th>
      <th>Key</th>
      <th>Mode</th>
      <th>Sales</th>
      <th>Streams</th>
      <th>Bpm</th>
      <th>Credits</th>
      <th>Structure</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= song.album %></td>
        <td><%= song.image_url %></td>
        <td><%= song.genre %></td>
        <td><%= song.key %></td>
        <td><%= song.mode %></td>
        <td><%= song.sales %></td>
        <td><%= song.streams %></td>
        <td><%= song.bpm %></td>
        <td><%= song.credits %></td>
        <td><%= song.structure %></td>
        <td><%= song.user_id %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
